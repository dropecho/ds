{
  "name": "com.vantreeseba.gameds",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "haxelib run munit t",
    "coverage": "haxelib run munit t -coverage",
    "watch": "npm-watch test",
    "watch-coverage": "npm-watch coverage"
  },
  "watch": {
    "test": {
      "patterns": [
        "test",
        "src"
      ],
      "extensions": "hx",
      "ignore": [
        "test/TestMain.hx",
        "test/TestSuite.hx"
      ]
    },
    "coverage": {
      "patterns": [
        "test",
        "src"
      ],
      "extensions": "hx",
      "ignore": [
        "test/TestMain.hx",
        "test/TestSuite.hx"
      ]
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "npm-watch": "^0.6.0"
  }
}
